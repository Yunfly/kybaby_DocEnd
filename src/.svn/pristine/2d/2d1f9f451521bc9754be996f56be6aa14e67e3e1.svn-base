/*申请成功*/
#result_error{
    margin-top: 20%;
    text-align: center;
}
#result_error>img{
    width: 50px;
}
#result_state{
    margin-top: 8px;
    line-height: 26px;
    font-size: 13px;
}
#big_button.big_btn_style,.big_button.big_btn_style{
    margin-top: 30px;
}
/*订单详情*/
#order_state{
    margin-left: 28px;
    padding-left: 28px;
    height: 43px;
    background: url(../../img/door_bespeak_open/order_status.png)no-repeat left center;
    background-size: 18px;
    line-height: 43px;
    color: #858585;
    font-size: 13px;
}
#pack_info{
    padding: 15px 28px;
    background: #fff;
    margin-bottom: 8px;
}
#pack_info>img{
    width: 50px;
    height: 50px;
}
#pack_name,#user_phone,#pack_name{
    margin-left: 10px;
}
#user_phone,#user_address{
    color: #858585;
    font-size: 13px;
}
#user_door_info{
    padding: 15px 8px;
    line-height: 24px;
}
.info_item{
    padding: 0 20px;
    line-height: 50px;
    background: #fff;
}
.info_item>div{
    padding: 0 8px;
    border-bottom: 1px solid #e8e8e8;
}
.info_item>div:last-child{
    border-bottom: 0px;
}
.details_info{
    text-align: right;
}
#choose_user{
    color: #858585;
    margin-right: 20px;
    font-size: 12px;
}
/*弹窗*/
#layer_box{
    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 1000;
    background: rgba(0,0,0,.3);
    display: none;
}
#layer_div{
    position: absolute;
    width: 80%;
    top:15%;
    left: 10%;
    height: 70%;
    max-height: 480px;
    background: #fff;
}
#layer_div>div{
    display: none;
}
.layer_title{
    background: #13b9d7;
    line-height: 45px;
    font-size: 17px;
    text-align: center;
    color: #fff;
}
#layer_div ul{
    overflow: auto;
}
.layer_list li,.layer_list_two li{
    padding: 0 18px;
    border-bottom: 1px solid #e8e8e8;
    line-height: 45px;
}
.bottom_button{
    margin: 10px 0;
    position: absolute;
    left: 10%;
    bottom: 0px;
    width: 80%;
    line-height: 35px;
    background: #fff;
    border: 1px solid #13b9d7;
    border-radius: 20px;
    color: #13b9d7;
    font-size: 17px;
    text-align: center;
}
/*选择出发地址*/
#reassign_record>li.s_selected{
    background: #fff url("../../img/door_bespeak_open/tick.png")no-repeat right 8px center;
    background-size: 16px;
}
.editor{
    margin-right: 10px;
    font-size: 12px;
    color: #13b9d7;
    border: 1px solid #13b9d7;
    border-radius: 10px;
    line-height: 24px;
    width: 38px;
    text-align: center;
}
/*地址信息*/
#address_area{
    padding: 0 20px;
}
#address_area>img{
    width: 100%;
}
#search_box{
    padding: 10px 20px;
}
#search_btn{
    width: 32px;
    height: 32px;
    background: url("../../img/door_bespeak_open/search.png")no-repeat right center;
    background-size: 21px;
}
#search_box p{
    border-radius: 20px;
    background: #fff;
    height: 32px;
    padding: 0 8px;
}
#search_box input{
    border: none;
    outline: none;
}
#address_list{
    padding: 0 20px;
    background: #fff;
}
#address_list>div{
    padding: 12px 8px;
    border-bottom: 1px solid #e8e8e8;
}
#address_list>div:last-child {
    border-bottom: 0px;
}
#address_list .small_gray{
    margin-top: 4px;
}
#progress_bar span{
    display: inline-block;
    width: 10px;
    height: 10px;
    position: absolute;
    bottom: -7px;
    left: -1px;
}
#progress_bar>div>div{
    padding: 6px 0;
    position: relative;
    color: #858585;
    font-size: 12px;
}
#progress_bar{
    padding: 8px 18px;
}
.green_now,.gray_before{
    border-bottom: 4px solid #E3E3E3;
    transition: .5s;
}
.green_after{
    border-bottom: 4px solid #13b9d7;
    transition: .5s;
}
#progress_bar>div>div:last-child {
    border-bottom: 4px solid #f4f4f4;
}
#progress_bar .green_now>span{
    bottom: -8px;
    width: 12px;
    height: 12px;
    background: url('../../img/door/sever_now.png')no-repeat left center;
    background-size: 12px;
}
.gray_before>span{
    background: url('../../img/door/sever_before.png')no-repeat left center;
    background-size: 10px;
}
.green_after>span{
    background: url('../../img/door/sever_after.png')no-repeat left center;
    background-size: 10px;
}
/*服务跟踪*/
#div2,#div3,#div4,#div5,#footer.server_trick,#button_two{
    display: none;
}
#div2{
    padding-top: 15px;
}
#go_type p{
    height: 50px;
    background: url("../../img/door_bespeak_open/round.png")no-repeat right 8px center;
    background-size: 18px;
}
#go_type p.selected{
    background: url("../../img/door_bespeak_open/true.png")no-repeat right 8px center;
    background-size: 18px;
}
#div3,#div4{
    padding: 10px 28px;
}
.big_title{
    margin: 10px 0px;
    font-size: 17px;
    color: #858585;
}
.server_content{
    line-height: 30px;
    color: #858585;
}
#button_two{
    margin-top: 10px;
    background: #f4f4f4;
    border: 1px solid #13b9d7;
    color: #13b9d7;
}
