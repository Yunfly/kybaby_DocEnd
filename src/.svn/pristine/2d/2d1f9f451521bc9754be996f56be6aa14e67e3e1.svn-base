body{
    background: #f4f4f4;
}
.door_open_list{
    padding: 0 20px;
    margin-bottom: 8px;
    background: #fff;
    position: relative;
    overflow: hidden;
}
.expired{
    width: 100px;
    height: 100px;
    position: absolute;
    left: -20px;
    top: -32px;
}

.door_open_list img{
    width: 50px;
    height: 50px;
}
.expired>img{
    width: 100px;
    height: 100px;
    opacity: 0.9;
    filter: alpha(opacity=90);
}
.server_time{
    padding: 10px 8px;
    font-size: 13px;
    /*overflow: hidden;*/
    /*white-space: nowrap;*/
    /*text-overflow: ellipsis;*/
    text-align: left;
    color: #858585;
}
.server_time>p{
    padding-left: 8px;
}
.server_time span{
    display: inline-block;
    font-size: 13px;
    margin-left: 10px;
}
.server_content,.left_distance{
    margin-left: 10px;
}
.server_address{
    font-size: 13px;
    color: #858585;
}
.server_info{
    padding: 15px 8px;
    border-bottom: 1px solid #e8e8e8;
}
.switch{
    position: relative;
    width: 44px;
    height: 24px;
    background: #EDEDED;
    border-radius: 50px;
    transition: .3s;
}
.switch.open{
    background: #13b9d7;
}
.switch>span{
    display: inline-block;
    width: 22px;
    height: 22px;
    background: url(../../img/door_bespeak_open/round_gray.png)no-repeat center;
    background-size: 22px;
    position: absolute;
    top: 2px;
    left: 0;
    transition: .3s;
}
.switch.open>span{
    left: 21px;
    background: url(../../img/door_bespeak_open/round_green.png)no-repeat center;
    background-size: 22px;
}
#footer_btn{
    width: 100%;
    height: 53px;
    background: #F4F4F4;
    position: fixed;
    bottom: 0;
}
#big_button,.big_button{
    margin: 0 auto;
    width: 86%;
    height: 49px;
    line-height: 49px;
    background: #13b9d7;
    color: #fff;
    font-size: 18px;
    text-align: center;
    border-radius: 50px;
}
/*选择出发地址*/
#set_icon>span{
    display: inline-block;
    width: 100%;
    height: 100%;
    background: url('../../img/door_bespeak_open/set.png')no-repeat right 20px center;
    background-size: 20px;
}
.info_item {
    padding: 0 20px;
    line-height: 50px;
    background: #fff;
}
.info_item>div {
    padding: 0 8px;
    border-bottom: 1px solid #e8e8e8;
}
.info_item>div:last-child {
    border-bottom: 0px;
}
.gray_title{
    font-size: 12px;
    color: #858585;
    padding: 8px 28px;
}
.small_gray{
    font-size: 12px;
    color: #858585;
}
.info_item>div.choose_address{
    padding: 12px 8px;
    line-height: 22px;
}
.choose_icon{
    display: inline-block;
    width: 18px;
    height: 18px;
}
.choose_icon.selected{
    background: url(../../img/door_bespeak_open/true.png)no-repeat right center;
    background-size: 18px;
}
.right_arrow{
    background: url('../../img/door_bespeak_open/next.png')no-repeat right 8px center;
    background-size: 9px;
}
.right_txt{
    margin-right: 20px;
    text-align: right;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: normal;
}
.service_week{
    width: 33.3%;
}
.service_hours li>div{
    border-bottom: 1px solid #e8e8e8;
    border-left: 1px solid #e8e8e8;
    height: 40px;
}
.service_hours li:last-child>div{
    border-bottom: none;
    border-left: 1px solid #e8e8e8;
}
.service_hours li{
    position: relative;
}
.service_hours li>div.service_del{
    width: 15px;
    height: 15px;
    border-radius: 18px;
    border: 1px solid #c0c0c0;
    line-height: 13px;
    text-align: center;
    color: #c0c0c0;
    position: absolute;
    top: -12px;
    right: -10px;
    font-size: 20px;
    background: #fff;
    font-weight: 100;
}
/*改派申请*/
.change_why{
    background: #fff;
    padding: 10px 20px;
}
.change_why>textarea{
    border: none;
    resize: none;
    width: 100%;
    height: 80px;
    outline: none;
    line-height: 24px;
}
/*新增地址*/
#delete{
    margin-top: 40px;
    display: none;
}
/*选择服务地址*/
.time_type{
    color: #858585;
}
.time_type>div{
    font-size: 13px;
}
.time_content,.time_type{
    text-align: center;
    line-height: 40px;
}
.time_content input{
    width: 100%;
    border: none;
    outline: none;
    text-align: center;
}
.has_line{
    border-left:1px solid #e8e8e8;
    border-right:1px solid #e8e8e8;
}
.add_time>p{
    margin: 0 auto;
    display: inline-block;
}
.add_time img{
    width: 17px;
}
.add_time span{
    margin-left: 3px;
}
#server_time>.info_item{
    margin-bottom: 8px;
}
/*上门订单列表*/
.order_list_info>p{
    line-height: 28px;
    font-size: 12px;
    color: #858585;
}
.order_list_img{
    width: 60px;
    overflow: hidden;
}
.order_list_img>img{
    width: 100%;
}
.order_list_data{
    margin: 8px 0 8px 10px;
    line-height: 23px;
}
.order_list_btn,.order_list_btn>div{
    overflow: hidden;
}
.order_list_btn>div{
    float: right;
}
.order_list_btn p{
    margin: 6px 0 6px 6px;
    float: left;
    border: 1px solid #13b9d7;
    color: #13b9d7;
    line-height: 24px;
    border-radius: 16px;
    width: 70px;
    text-align: center;
    font-size: 13px;
}
.order_list_data>li.lise_order_name{
    font-size: 15px;
    color: #000;
}
.order_list_data>li,.lise_order_phone{
    font-size: 13px;
    color: #858585;
}
#reassign_record>li{
    margin-bottom: 8px;
    background: #fff;
    padding: 15px 20px;
}
.record_info{
    margin-top: 8px;
}
li.selected{
    background: url("../../img/door_bespeak_open/tick.png")no-repeat right 8px center;
    background-size: 16px;
}
li.select{
    background: url("../../img/door_bespeak_open/choose.png")no-repeat right 8px center;
    background-size: 16px;
}
