<script>
  // require('../../public/css/animate.css')
  import $j from 'jquery'
  export default{
    data(){
      return{
        orders:'',
        childOrders:'',
        liState:'全部',
        orderType:'儿科',
        isActive:0,
        index:0,
        idx:0
      }
    },
    created: function () {
    },
    mounted:function(){
      var that = this;
      this.$ajax.post(urlWay.clinicHost+'doctorClinic.action',
        'action=getDoctorMorePracticeList&doctorMorePractice.clinicOrgType=1')
        .then(function(result){
          that.orders = result.data.doctorMorePracticeList;
        })
        .catch(function (error) {
          console.log(error);
        });
      this.$ajax.post(urlWay.fdmanageHost+'fdService.action',
        'action=getOrganChildcareOpenDoctorByDoctor')
        .then(function(result){
          that.childOrders = result.data.organChildcareOpenDoctorList;
        })
        .catch(function (error) {
          console.log(error);
        });
    },
    methods: {
      change:function(state,index){
        this.idx = index;
        if(state=="未完成"){
          this.liState="已完成";
          $(this).css('left',"20px");
        }else if(state=="已完成"){
          this.liState="未完成";
        }else{
          this.liState="全部";
        }
      },
      typeChange:function(state,index){
        this.isActive=index;
        if(state=="儿科"){
          this.orderType="儿科";
          this.index=0;
        }else{
          this.orderType="儿保";
          this.index=1;
        }
      }
    }
  }
</script>
