#container_full{
    padding:0 18px;
}
.order_item .flex_item:nth-child(1){
    flex:0 0 20%;
}
.order_item .flex_item:nth-child(2){
    padding-left:20px;
}
.order_item img{
    border-radius: 50%;
}
.order_title div{
    padding:5px 0;
}
.flex_item dl{
    padding-bottom:5px;
}
.flex_item dl dd,.flex_item dl dt{
    line-height: 2rem;
}
.list_btn .btn{
    height:29px;
}
.list_btn{
    padding:5px 0;
}